def create_graph(n):
    print("Creating Graph")

    counter = 1
    for x in range(0,n):

        for y in range(0,n):
            # Does not go up
            if x == 0:
                # Does not go left AND does not go up
                if y == 0:
                    pass
                # Does not go right AND does not go up
                elif y == n-1:
                    pass
                # General case, Does not go up AND goes left AND right
                else:
                    pass
            # Does not go down
            elif x == n-1:
                pass
            # General case
            else:
                pass


